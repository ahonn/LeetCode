{
  "parserOptions": {
    "ecmaVersion": 2016,
  },
  "env": {
    "node": true,
  },
  "extends": "airbnb-base",
  "globals": {
    "ListNode": true,
  },
  "rules": {
    "no-unused-vars": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-bitwise": 0,
    "no-multi-assign": 0,
    "max-len": 0,
    "prefer-destructuring": 0,
    "no-restricted-syntax": 0,
    "no-continue": 0
  }
}

